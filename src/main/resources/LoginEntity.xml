<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.wangzn.entity.LoginEntity">
        <table name="login" schema="center" catalog=""/>
        <attributes>
            <basic name="id">
                <column name="id" precision="11"/>
            </basic>
            <basic name="name">
                <column name="name" length="20"/>
            </basic>
            <basic name="password">
                <column name="password" length="100"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
